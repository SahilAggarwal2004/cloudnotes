{
  "name": "cloudnotes",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "aos": "^2.3.4",
    "axios": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.16.0",
    "react-text-to-speech": "^0.4.3",
    "swr": "^2.2.2",
    "web-vitals": "^3.4.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-recipes": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-window": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn run watch:css && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "watch:css": "postcss src/tailwind.css -o src/index.css",
    "watch": "chokidar \"./src/**/*.js\" -c \"yarn run watch:css\"",
    "dev": "concurrently \"yarn run watch\" \"yarn run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.1",
    "postcss": "^8.4.30",
    "postcss-cli": "^10.1.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "workbox-webpack-plugin": "^7.0.0"
  }
}
