{
  "name": "cloudnotes",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "aos": "^2.3.4",
    "axios": "^0.26.1",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.2",
    "react-text-to-speech": "^0.3.0",
    "swr": "^1.3.0",
    "web-vitals": "^2.1.4",
    "workbox-core": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-recipes": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3",
    "workbox-window": "^6.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn run watch:css && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "watch:css": "postcss src/tailwind.css -o src/index.css",
    "watch": "chokidar \"./src/**/*.js\" -c \"yarn run watch:css\"",
    "dev": "concurrently \"yarn run watch\" \"yarn run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.0",
    "tailwindcss": "^3.0.23",
    "workbox-webpack-plugin": "^6.5.3"
  }
}
